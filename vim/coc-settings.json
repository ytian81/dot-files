{
    "cSpell.spellCheckDelayMs": 500,
    "clangd.arguments": [
        "--background-index",
        "--clang-tidy",
        "--compile-commands-dir=${workspaceFolder}/build/bin",
        // "--enable-config",
        "--malloc-trim",
        "--pretty",
        "--project-root=${workspaceFolder}",
        "--remote-index-address=0.0.0.0:9090"
    ],
    "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/14.0.3/clangd_14.0.3/bin/clangd",
    // "clangd.trace.file": "/tmp/clangd.log",
    // "clangd.trace.server": "verbose",
    "coc.preferences.currentFunctionSymbolAutoUpdate": true,
    "coc.preferences.enableMessageDialog": true,
    "colors.filetypes": ["vim"],
    "floatFactory.floatConfig": {
        "border": true,
        "borderhighlight": "Comment",
        "focusable": false,
        "highlight": "Normal",
        "maxHeight": 120,
        "maxWidth": 100,
        "rounded": true,
        "winblend": 10
    },
    "diagnostic.floatConfig": {
        "border": true,
        "borderhighlight": "Comment",
        "focusable": false,
        "highlight": "Normal",
        "maxHeight": 120,
        "maxWidth": 100,
        "rounded": true,
        "winblend": 10
    },
