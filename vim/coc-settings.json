        "maxHeight": 120,
        "maxWidth": 100,
        "rounded": true,
        "winblend": 10
    },
    "diagnostic.floatConfig": {
        "border": true,
        "borderhighlight": "Comment",
        "focusable": false,
        "highlight": "Normal",
        "maxHeight": 120,
        "maxWidth": 100,
        "rounded": true,
        "winblend": 10
