{
    "cSpell.spellCheckDelayMs": 500,
    "clangd.arguments": [
        "--background-index",
        "--clang-tidy",
        "--compile-commands-dir=${workspaceFolder}/build/bin",
        // "--enable-config",
        "--malloc-trim",
        "--pretty",
        "--project-root=${workspaceFolder}",
        "--remote-index-address=0.0.0.0:9090"
    ],
    "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/18.1.3/clangd_18.1.3/bin/clangd",
    // "clangd.trace.file": "/tmp/clangd.log",
    // "clangd.trace.server": "verbose",
    "inlayHint.display": false,
    "coc.preferences.currentFunctionSymbolAutoUpdate": true,
    "coc.preferences.enableMessageDialog": false,
    "coc.source.emoji.filetypes": [
        "markdown",
        "gitcommit"
    ],
    "coc.source.around.priority": 100,
    "suggest.languageSourcePriority": 95,
    "diagnostic.errorSign": "✘",
    "diagnostic.warningSign": "󱈸",
    "diagnostic.infoSign": "󰙎",
    "colors.filetypes": [
        "vim"
    ],
    "floatFactory.floatConfig": {
